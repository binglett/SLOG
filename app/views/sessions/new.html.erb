<div id="login-center">

	<h1>Sign In</h1>
	<div class="form-space">
		<div class="forms" id="signin-form">
			<% if flash[:error] %>
				 <p><%= flash[:error] %></p>
			<% end %>
		  <%= simple_form_for(:session, :url =>sessions_path)  do |f| %>
				<%= f.input :email %>
				<%= f.input :password %>
				<%= f.button :submit, "Sign In" %>
			<% end %>
		</div>
	</div>
</div>
